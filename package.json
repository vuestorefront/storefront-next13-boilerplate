{
  "name": "@vsf/react-boilerplate",
  "version": "1.0.1",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,css,scss,md}\"",
    "clean": "npx rimraf ./**/node_modules && rm yarn.lock",
    "clean:hard": "npx rimraf ./**/node_modules && npx rimraf ./**/lib && rm yarn.lock",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:coverage": "npx turbo run test:coverage"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^8.38.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.1",
    "@vue-storefront/eslint-config": "^2.0.0-rc.5",
    "babel-cli": "^6.26.0",
    "babel-preset-node": "^5.1.1",
    "commitizen": "^4.2.5",
    "cypress": "^10.8.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "latest",
    "rollup": "^2.79.0",
    "ts-jest": "^29.0.3",
    "turbo": "1.5.5",
    "typescript": "4.8.2"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.10",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "disableScopeLowerCase": true
    }
  }
}
